    <joint name="joint_fcamera_stick" type="fixed">
        <origin xyz="-0.450 0 0.12" rpy="0 0 0"/>
        <parent link="r750"/>
        <child link="camera_stick"/>
    </joint>


    <joint name="joint_fcamera_holder" type="fixed">
        <origin xyz="0.05 0 1.27" rpy="0 0 ${PI/2}"/>
        <parent link="camera_stick"/>
        <child link="camera_holder"/>
    </joint>

    <xacro:xtion parent="r2_link_6" suffix="2" prefix="" color="grey">
        <origin xyz="-0.13 -0.15 0" rpy="0 ${-PI/2} 0"/>
    </xacro:xtion>
    <xacro:xtion parent="r1_link_6" suffix="1" prefix="" color="grey">
        <origin xyz="-0.13 -0.15 0" rpy="0 ${-PI/2} 0"/>
    </xacro:xtion>

    
<!-- table r2 -->
    <xacro:table parent="base_link" suffix="t2_" color="grey" height="0.72" deskx="0.8" desky="0.8">
        <origin xyz="1.3 0 0.72" rpy="0 0 0"/>
    </xacro:table>


<!-- table r1 -->
    <xacro:table parent="base_link" suffix="t1_" color="grey" height="0.72" deskx="0.8" desky="1.5">
        <origin xyz="-1.15 0 0.72" rpy="0 0 0"/>
    </xacro:table>

<!-- front table  -->
 <!--   <xacro:table parent="base_link" suffix="t3_" color="grey" height="0.72" deskx="0.8" desky="0.8">
