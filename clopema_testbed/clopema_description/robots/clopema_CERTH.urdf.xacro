<xacro:alvar_marker parent="r1_tip_link" suffix="r1" width="0.052">
        <origin xyz="-0.015 0.0259 0.065" rpy=" ${PI/6} ${-PI/2} ${-PI/2} "/>
    </xacro:alvar_marker>

