<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="clopema">
    <include filename="$(find clopema_description)/urdf/common.urdf.xacro" />
    <include filename="$(find
clopema_description)/urdf/room_certh.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/base_link.urdf.xacro" />
    <include filename="$(find
clopema_description)/urdf/ma1400/ma1400.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/gripper.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/r750.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/table_adjustable.urdf.xacro" />
    <include filename="$(find
clopema_description)/urdf/colObjects.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/dx100.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/xtion3.urdf.xacro" />
    <include filename="$(find clopema_description)/urdf/xtion.urdf.xacro" />
	<xacro:include filename="$(find clopema_description)/urdf/alvar_marker.urdf.xacro" />
<xacro:include filename="$(find clopema_description)/urdf/robot_head_UG.xacro" />

    <xacro:base_link/>
    <xacro:room_certh/>
    <xacro:r750 parent="base_link" suffix="" color="blue">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </xacro:r750>

    <xacro:ma1400 parent="r750" suffix="r1_" color="blue">
        <origin xyz="0 -.25 0.1600" rpy="${15*TO_RAD} 0 0"/>
    </xacro:ma1400>
    <xacro:ma1400 parent="r750" suffix="r2_" color="blue">
        <origin xyz="0 .25 0.1600" rpy="-${15*TO_RAD} 0 0"/>
    </xacro:ma1400>

    <xacro:gripper parent="tip_link" suffix="r1_" color="blue">
        <origin xyz="0.000 0 0.02" rpy="0 0 ${PI/2+PI/6}"/>
    </xacro:gripper>
    <xacro:gripper parent="tip_link" suffix="r2_" color="blue">
        <origin xyz="0.000 0 0.02" rpy="0 0 ${PI/2+PI/6}"/>
    </xacro:gripper>


    <link name="box_pc">
        <visual>
            <geometry>
                <box size="1.0 1.00 0.6" />
            </geometry>
            <material name="grey" />
            </visual>
        <collision>
            <geometry>
                <box size="1.0 1.00 0.6" />
            </geometry>
        </collision>
    </link>
    
    <joint name="joint_fbox_pc" type="fixed">
        <origin xyz="0 1.0 0.3" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="box_pc"/>
    </joint>s
    
    <link name="cable_holder">
        <visual>
            <geometry>
                <box size="0.4 0.40 0.40" />
            </geometry>
            <material name="grey" />
            </visual>
        <collision>
            <geometry>
                <box size="0.4 0.40 0.40" />
            </geometry>
        </collision>
    </link>
    
    <joint name="joint_fcable_holder" type="fixed">
        <origin xyz="0 1.6 2.0" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="cable_holder"/>
    </joint>

    <link name="camera_stick">
        <visual>
            <geometry>
                <box size="0.1 0.15 1.6" />
            </geometry>
            <material name="grey" />
            <origin xyz="-0.00 0 0.80"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.1 0.15 1.6" />
            </geometry>
            <origin xyz="-0.00 0 0.80"/>
        </collision>
    </link>

    <link name="camera_holder">
        <visual>
            <geometry>
                <box size="0.04 0.20 0.04" />
            </geometry>
            <origin xyz="0 -0.1 0" rpy="0 0 0"/>
            <material name="grey" />
            </visual>
        <collision>
            <geometry>
                <box size="0.04 0.20 0.04" />
            </geometry>
            <origin xyz="0 -0.1 0" rpy="0 0 0"/>
        </collision>
    </link>

